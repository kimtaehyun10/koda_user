<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <!-- Globally enables or disables any caches configured in any mapper under this configuration -->
        <setting name="cacheEnabled" value="true"/>
        <!-- Sets the number of seconds the driver will wait for a response from the database -->
        <setting name="defaultStatementTimeout" value="3000"/>
        <!-- Enables automatic mapping from classic database column names A_COLUMN to camel case classic Java property names aColumn -->
        <setting name="mapUnderscoreToCamelCase" value="true"/>
        <!-- Allows JDBC support for generated keys. A compatible driver is required.
        This setting forces generated keys to be used if set to true,
         as some drivers deny compatibility but still work -->
        <setting name="useGeneratedKeys" value="true"/>

        <setting name="jdbcTypeForNull" value="NULL" />
        <setting name="callSettersOnNulls" value="true"/>
    </settings>

    <typeAliases>
        <typeAlias alias="egovMap" type="egovframework.rte.psl.dataaccess.util.EgovMap"/>
        <typeAlias alias="storyBean" type="com.koticobui.dfmc.client.donate.model.StoryVO"/>
        <typeAlias alias="donateBean" type="com.koticobui.dfmc.client.donate.model.DonateVO"/>
        <typeAlias alias="donateSearchBean" type="com.koticobui.dfmc.client.donate.model.DonateSearchVO"/>
        <typeAlias alias="PublicSearch" type="com.koticobui.dfmc.client.pb.model.PublicSearchVO"/>
        <typeAlias alias="BrainSearch" type="com.koticobui.dfmc.client.pb.model.BrainSearchVO"/>
    </typeAliases>

    <!-- Continue going here -->

</configuration>